<UserControl x:Class="MvvmProj.Views.UserControls.SpecialUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MvvmProj.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="150" Height="150" Width="150" Margin="10,10,0,0">
    <Grid Background="Red" >
        <Button Command="{Binding SelectUCCommand}" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"
               Content="{Binding PrinterModel}"
               ></Button>
    </Grid>
</UserControl>
